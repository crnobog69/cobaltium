{
  "manifest_version": 2,
  "name": "Cobaltium",
  "version": "1.3.1",
  "description": "Једноставан `Cobalt.tools` додатак. ",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["tabs", "<all_urls>"],
  "browser_action": {
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "homepage_url": "https://github.com/crnobog69/cobaltara",
  "author": "Црнобог | Crnobog"
}
